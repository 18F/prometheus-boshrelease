check process prometheus
  with pidfile /var/vcap/sys/run/prometheus/prometheus.pid
  start program "/var/vcap/jobs/prometheus/bin/monit_debugger prometheus_ctl '/var/vcap/jobs/prometheus/bin/prometheus_ctl start'"
  stop program "/var/vcap/jobs/prometheus/bin/monit_debugger prometheus_ctl '/var/vcap/jobs/prometheus/bin/prometheus_ctl stop'"
  group vcap
