#!/bin/bash

set -eux

# Copy common utils
mkdir -p ${BOSH_INSTALL_TARGET}/common
cp -a ${BOSH_COMPILE_TARGET}/common/* ${BOSH_INSTALL_TARGET}/common

# Extract grafana package
tar xzvf ${BOSH_COMPILE_TARGET}/grafana/grafana-4.0.0-1480439068.linux-x64.tar.gz
cp -a ${BOSH_COMPILE_TARGET}/grafana-4.0.0-1480439068/* ${BOSH_INSTALL_TARGET}
